{
  "functions": {
    "pages/api/sitemap.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/sitemap",
      "schedule": "0 3 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/pages/api/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev"
}